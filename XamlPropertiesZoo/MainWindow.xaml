<Window x:Class="XamlPropertiesZoo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XamlPropertiesZoo"
        xmlns:models="clr-namespace:XamlPropertiesZoo.Models"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="MainWindow" Height="240" Width="534">
  <Grid>
    <ListBox x:Name="lstZoo" HorizontalAlignment="Left" Height="83" Margin="10,10,0,0" VerticalAlignment="Top" Width="500">
      <models:Zoo>
        <models:Zoo.Location>
          <models:Location City="Wien">
            <models:Location.Address>
              <models:Address StreetName = "Hietzing" StreetNr = "66"></models:Address>
            </models:Location.Address>
          </models:Location>
        </models:Zoo.Location>
        <models:Zoo.Animals>
          <models:Animal Name="Tiger" Species="Raubkatze">
            <models:Animal.Ages>
              <sys:Int32>6</sys:Int32>
              <sys:Int32>6</sys:Int32>
              <sys:Int32>3</sys:Int32>
            </models:Animal.Ages>
          </models:Animal>
          <models:Animal Name = "Panda" Species = "Baer">
            <models:Animal.Ages>
              <sys:Int32>6</sys:Int32>
              <sys:Int32>3</sys:Int32>
            </models:Animal.Ages>
          </models:Animal>
        </models:Zoo.Animals>
      </models:Zoo>
